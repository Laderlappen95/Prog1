package packet;

public class Lucka1 {

	public static void main(String[] args) {
		int tal = 0;
		int tal1 = 0;
		int tal2 = 0;
		for(int i = 0; 140>=i; i++) {
			tal = tal +  127 + i;
		}
		for(int i = 0; 268 >=i; i++) {
			tal1 = tal1 + i + 1110;
		}
		for(int i = 0; 37 >= i; i++) {
			tal2 = tal2 + i + 3239293;
		}
		int tot = tal + tal1 + tal2;
		System.out.println(tot);
		
	}

}
