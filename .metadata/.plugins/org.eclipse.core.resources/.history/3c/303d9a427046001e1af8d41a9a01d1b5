package pu1;

import java.util.Scanner;

public class Uppg1 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.println("Skriv in l채ngd i cm:");
		
		int l채ngd = input.nextInt();
		
		System.out.println("Skriv in bredd i cm:");
		
		int bredd = input.nextInt();
		
		System.out.println("Arean 채r: "+ bredd * l채ngd " cm^2");
		
		

	}

}
